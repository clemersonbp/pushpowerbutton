{
  "name": "pushpowerbutton",
  "displayName": "PushPowerButton",
  "description": "A quick shortcut to send files to OCC via DCU command.",
  "version": "0.0.1",
  "icon": "./resources/icon.png", 
  "publisher": "Clemerson Pessalli",
  "author": {
    "name": "Clemerson Pessalli",
    "email": "clemersonpessalli@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clemersonbp/pushpowerbutton"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pushpowerbutton.pushFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pushpowerbutton.pushFile",
        "title": "Push File",
        "category": "File",
        "icon": {
          "dark": "./resources/lightning.png",
          "light": "./resources/lightning-light.png"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "pushpowerbutton.pushFile",
          "group": "navigation",
          "when": "editorIsOpen"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "pushFileButton",
          "name": "Push File"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}